{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "outDir": "./dist", 
    "rootDir": "./src",
    "removeComments": true, 
    "strict": true, 
    "noImplicitAny": true, 
    "noEmitOnError": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
}