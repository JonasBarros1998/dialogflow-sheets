{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "new-cap": ["error", {"capIsNew": false}],
        "max-len": ["error", { "code": 90 }],
        "require-jsdoc": ["error", {
            "require": {
                "ClassDeclaration": false,
                "MethodDefinition": false
            }
        }]
    }
}
